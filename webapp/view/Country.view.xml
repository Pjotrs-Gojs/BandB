<mvc:View
	controllerName="com.BandB.BandB.controller.Country"
	xmlns:l="sap.ui.layout"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<List
		id="Country"
		items="{/Country}"
		itemPress="onPress">
		<StandardListItem
			title="{COUNTRY}"
			icon="https://cdn.britannica.com/49/6249-004-179BDE49/Flag-Latvia.jpg"
			type="Active"/>
	</List>
</mvc:View>