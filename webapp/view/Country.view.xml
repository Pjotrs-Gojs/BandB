<mvc:View
	controllerName="com.BandB.BandB.controller.Country"
	xmlns:l="sap.ui.layout"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<List
		id="Country"
		items="{/Country}">
		<StandardListItem
			title="{COUNTRY}"
			icon="https://media.gettyimages.com/photos/riga-cathedral-riga-latvia-picture-id1092243322?s=612x612"/>
	</List>
</mvc:View>